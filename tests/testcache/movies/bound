			<script type="text/javascript">
				
				nflxFucntions.setNflxBtns = function()
				{
					if( false )
					{
						nflxFucntions.showSearchLink( "Bound" );
					}
					else if( false )
					{
						nflxFucntions.showAddPlayBtn();
					}
					else if( true ) 
					{
						nflxFucntions.showAddBtn();
					}
					else if ( false )
					{
						nflxFucntions.showPlayBtn();
						
					}
					
				}
				
				nflxFucntions.showAddPlayBtn = function()
				{
					document.getElementById('_nflxAddButton').style.display = "none";
					document.getElementById('_nflxPlayButton').style.display = "none";
					document.getElementById('_nflxAddPlayButton').style.display = "inline";
					document.getElementById('_nflxSearchLink').style.display = "none"
				}
				
				nflxFucntions.showAddBtn = function()
				{
					document.getElementById('_nflxAddButton').style.display = "inline";
					document.getElementById('_nflxPlayButton').style.display = "none";
					document.getElementById('_nflxAddPlayButton').style.display = "none";
					document.getElementById('_nflxSearchLink').style.display = "none"
				}
				nflxFucntions.showPlayBtn = function()
				{
					document.getElementById('_nflxAddButton').style.display = "none";
					document.getElementById('_nflxPlayButton').style.display = "inline";
					document.getElementById('_nflxAddPlayButton').style.display = "none";
					document.getElementById('_nflxSearchLink').style.display = "none"
				}
				nflxFucntions.showSearchLink = function( displayTitle )
				{
					document.getElementById('_nflxAddButton').style.display = "none";
					document.getElementById('_nflxPlayButton').style.display = "none";
					document.getElementById('_nflxAddPlayButton').style.display = "none";
					document.getElementById('_nflxSearchLink').href = "http://www.netflix.com/Search?ff2_submit.x=&ff2_submit.y=&v1=" + displayTitle;
					document.getElementById('_nflxSearchLink').style.display = "inline"
					
				}
				nflxFucntions.setNflxID = function( nflxID )
				{
					nflxFucntions.nflxID = nflxID;
				}
				
				nflxFucntions.setNflxBtns();
				nflxFucntions.setNflxID( "326674" );
				
			</script></div></div>
