		$(document).ready(function()
		{
			setSiteWidth();
			
			searchBox_showPlaceHolder();
			
			var u = "";
			if ("false" == "true" || u != "")
			{
				if (u != "")
				{
					login_show(u);
				}
				else
				{
					login_show("");
				}
			}
			
			FBCCF.init();
			
			//fix for onkeyup event, because if autocomplete selected than onkeyup will not execute
			$("[onkeyup]").each(function()
			{
			    $(this).attr("autocomplete", "off");
			});
			
			//fix for onmouseout event in contentStrip rollover
			$("#contentStrip_rollover").mouseleave(function()
			{
				contentStrip_rolloverHide();
			});
			
			
			//search box
			
			$("#tabs").tabs();
			
			$("#searchBox_input").attr("autocomplete", "off");
			
			searchBox_suggestionsPanel = document.getElementById("searchBox_suggestions");
			
			searchBox_updateMoodsButton($("#searchBox_moodsResultsCount").val());
			
			$("#searchBox_form").bind("keyup", function(e)
			{
				var code = e.keyCode || e.which; 
				if (code == 13)
				{
					e.preventDefault();
					return false;
				}
			});
			
			$("#searchBox_form").bind("keypress", function(e)
			{
				var code = e.keyCode || e.which; 
				if (code == 13)
				{
					e.preventDefault();
					return false;
				}
			});
	
		right_switchShare("Review");
		ga('send', 'event', 'title', 'content-page', 'bound');
		});
		
		$(window).resize(function()
		{
			$(".popup:visible").each(function()
			{
				popup_center($(this).attr("id"));
			});
			
			setSiteWidth();
		});
	
